pipeline {
    agent any

stages {
    stage ('Parallel Stage') {
        parallel {
            stage ('Stage-1')
            {
                steps {
                    echo "Welcome to Batch-29!"
                    sleep 5
                }
            }
            stage ('Stage-2')
            {
                steps{
                   echo "Keep Learning!"
                   sleep 5 
                }
            }
            stage ('Stage-3')
            {
                steps{
                    echo "Keep Growing!"
                    sleep 5
                }
            }

        }
    }
}
}

